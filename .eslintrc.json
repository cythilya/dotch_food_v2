{
  "env": {
    "browser": true,
    "es6": true,
    "amd": true,
    "commonjs": true,
    "node": true,
    "jasmine": true
  },
  "globals": {
    "React": true,
    "const": true,
    "ga": true
  },
  "extends": [
    "eslint:recommended",
    "airbnb"
  ],
  "parserOptions": {
    "ecmaVersion": 7,
    "sourceType": "module",
    "ecmaFeatures": {
      "modules": true,
      "jsx": true,
      "experimentalObjectRestSpread": true
    }
  },
  "plugins": [
    "react"
  ],
  "rules": {
    "no-underscore-dangle": "off",
    "arrow-body-style": "off",
    "no-param-reassign": [
      "error",
      {
        "props": false
      }
    ],
    "max-len": [
      "error",
      140
    ],
    "consistent-return": "off",
    "padded-blocks": "off",
    "new-cap": "off",
    "react/no-string-refs": "error",
    "react/jsx-filename-extension": [
      1,
      {
        "extensions": [
          ".js",
          ".jsx"
        ]
      }
    ],
    "jsx-a11y/no-static-element-interactions": 0,
    "react/forbid-prop-types": 0
  }
}
